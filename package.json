{
  "name": "qna-maker-kb-action",
  "version": "0.1.0",
  "description": "QnA Maker Knowledgebase Action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint ts/**/*.ts",
    "package": "ncc build ts/index.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "jest": {
    "clearMocks": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmservera/qna-maker-kb-action"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Juan Manuel Servera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions/javascript-action/issues"
  },
  "homepage": "https://github.com/actions/javascript-action#readme",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@azure/cognitiveservices-qnamaker": "^3.2.0",
    "@azure/ms-rest-js": "^2.6.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.13.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.0.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.2.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}